    "@aws-sdk/url-parser": "^3.357.0",
    "@aws-sdk/util-format-url": "^3.363.0",
    "@google-cloud/translate": "^8.0.1",
    "@nestjs/apollo": "^12.0.7",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/common": "^10.0.5",
    "@nestjs/core": "^10.0.5",
    "@nestjs/graphql": "^12.0.7",
    "@nestjs/platform-express": "^10.0.5",
    "@nestjs/serve-static": "^4.0.0",
    "@octokit/rest": "^20.0.2",
    "@types/argon2": "^0.15.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/pg": "^8.10.2",
    "adm-zip": "^0.5.10",
    "apollo-server-core": "^3.12.0",
    "apollo-server-express": "^3.12.0",
    "argon2": "^0.30.3",
    "axios": "^1.6.0",
    "deepl-node": "^1.10.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "graphql": "^16.7.1",
    "graphql-subscriptions": "^2.0.0",
    "graphql-upload-ts": "^2.0.9",
    "html-to-text": "^9.0.5",
    "jsonwebtoken": "^9.0.1",
    "language-tags": "^1.0.8",
    "nanoid": "^3.3.0",
    "node-fetch": "2",
    "node-html-parser": "^6.1.10",
    "openai": "^4.12.0",
    "pg": "^8.11.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "svgson": "^5.3.1",
    "temp": "^0.9.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.9",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.1.0",
    "@types/adm-zip": "^0.5.2",
    "@types/express": "^4.17.13",
    "@types/html-to-text": "^9.0.1",
    "@types/jest": "28.1.8",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@types/temp": "^0.9.2",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.3",
    "lint-staged": "^13.2.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  },
  "lint-staged": {
    "**/*.{ts,tsx,md,mdx,js,jsx}": [
      "prettier --write",
      "eslint --cache --fix"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "packageManager": "yarn@3.6.3"
}
